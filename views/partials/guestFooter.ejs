  <script>
  // cookieBanner.js
    document.addEventListener('DOMContentLoaded', () => {
    const cookieBanner = document.getElementById('cookieBanner');
    const acceptCookiesBtn = document.getElementById('acceptedCookies');
    const declineCookiesBtn = document.getElementById('declineCookies');

    const userAcceptedCookies = document.cookie.includes('consentCookie=true');

    if (!userAcceptedCookies) {
        cookieBanner.style.display = 'block';
    }

    acceptCookiesBtn.addEventListener('click', () => {
        document.cookie = 'consentCookie=true; max-age=' + (365 * 24 * 60 * 60);
        cookieBanner.style.display = 'none';
		window.location.reload();
    });

    declineCookiesBtn.addEventListener('click', () => {
        alert('By declining, you may not have access to the site.');
        // Additional logic if needed
    });
});

  </script>

